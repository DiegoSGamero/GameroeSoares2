<div class="navbar navbar-expand{-sm|-md|-lg|-xl|-xxl} p-1 d-flex row">

  <div class="logo col-2">
    <%= link_to root_path, class: "navbar-brand m-0" do %>
      <%= image_tag "lgotipo_gameroesoares-removebg-preview.png", class: "logotipo" %>
    <% end %>
  </div>
  <div class="col-8 d-flex justify-content-between">
    <div class="nav-item p-1">
      <%= link_to "Produtos", "/produtos", class: "links_navbar" %>
      <%# <p> Produtos </p> %>
    </div>
    <div class="nav-item p-1">
      <%= link_to "Conhecimento", "/conhecimento", class: "links_navbar" %>
    </div>
    <div class="nav-item p-1">
      <%= link_to "Contato", "/contato", class: "links_navbar" %>
    </div>
    <div class="nav-item p-1">
      <%= link_to "Trabalhos", "/trabalhos", class: "links_navbar" %>
    </div>
    <div class="nav-item p-1">
      <%= link_to "Cadastre-se", new_user_registration_path, class: "sign_up_bar" %>
    </div>
  </div>
  <% if user_signed_in? %>
    <div class="col-2">
      <div class="btn login-home ms-5 p-1 text-center">
        <%= link_to "Sair", destroy_user_session_path, data: {turbo_method: :delete}, class: "sign_up_bar" %>
      </div>
    </div>
  <% else %>
  <div class="col-2 d-flex justify-content-center">
    <div class="btn login-home p-1 text-center">
      <%= link_to "Entrar", new_user_session_path, class: "sign_up_bar" %>
    </div>
    <div class="btn login-home p-1 text-center ms-3">
      <%= link_to "Admin", new_admin_session_path, class: "sign_up_bar" %>

    </div>
  </div>
  <% end %>
</div>
